generator client {
  provider      = "prisma-client-js"
  output        = "../../../node_modules/@prisma/client-uniswap"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATA_ANALYSIS_DATABASE_URL")
}

model SwapVolume {
  id            String   @id @default(cuid())
  timestamp     DateTime
  volume        Float
}